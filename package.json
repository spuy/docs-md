    "build:webpack-debug": "cross-env DEBUG=vuepress* vuepress-webpack build src",
    "dev:vite": "vuepress-vite dev src",
    "dev:vite-clean": "vuepress-vite dev src --clean-cache",
    "dev:vite-debug": "cross-env DEBUG=vuepress* vuepress-vite dev src",
    "dev:webpack": "vuepress-webpack dev src",
    "dev:webpack-clean": "vuepress-webpack dev src --clean-cache",
    "dev:webpack-debug": "cross-env DEBUG=vuepress* vuepress-webpack dev src",
    "lint": "prettier --check --write .",
    "lint:check": "prettier --check .",
    "lint-md": "markdownlint **/*.md --ignore node_modules --fix",
    "test": "echo \"Warning: no test specified\" && exit 0"
  },
  "author": "Mr.Hope",
  "license": "MIT",
  "keywords": [
    "vuepress",
    "personal-blog"
  ],
  "dependencies": {
    "@vuepress/cli": "2.0.0-beta.63",
    "@vuepress/client": "2.0.0-beta.63",
    "@vuepress/core": "2.0.0-beta.63",
    "@vuepress/shared": "2.0.0-beta.63",
    "@vuepress/utils": "2.0.0-beta.63",
    "axios": "0.27.2",
    "pnpm": "8.6.5",
    "sass-loader": "13.3.2",
    "vue": "3.3.4",
    "vue-router": "4.2.2",
    "vuepress": "2.0.0-beta.63",
    "vuepress-plugin-blog2": "2.0.0-beta.228",
    "vuepress-plugin-comment2": "2.0.0-beta.228",
    "vuepress-theme-hope": "2.0.0-beta.228",
    "vuepress-vite": "2.0.0-beta.63",
    "vuepress-webpack": "2.0.0-beta.63"
  },
  "devDependencies": {
    "@vuepress/plugin-search": "2.0.0-beta.63",
    "cross-env": "7.0.3",
    "markdownlint-cli": "0.31.1",
    "prettier": "2.6.2",
    "typescript": "^5.7.2"
  },
  "packageManager": "pnpm@9.1.1",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/react",
        "react",
        "react-dom",
        "webpack"
      ]
    }
